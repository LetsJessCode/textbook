<h1>Login</h1>

<%= form_for @user, url: login_path do |f| %>
<div>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>

   <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
 
   <%= f.submit 'Login'%>
   <% end %> <br><br>
   <%= button_to "Log in with Google", '/auth/google_oauth2' %><br/>
    </div>