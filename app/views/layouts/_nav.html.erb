     
<header class="container">
  <nav class="navbar navbar-default">
    <div class="container-fluid">
      <div class="navbar-header">
        <%=link_to 'Home', root_path, class: "navbar-brand" %>
        <%=link_to 'View Courses', courses_path, class: "navbar-brand" %>
      </div>
      
      <ul class="nav navbar-nav navbar-right">    
      <% if logged_in? %>  
         <%# binding.pry %>
        <li><%=link_to 'Edit UserProfile', edit_user_path(current_user) %></li>
        <li><%=link_to 'Add Assignment', new_user_assignment_path(current_user) %></li>  
        <li><%=link_to 'Your Homework', user_assignments_path(current_user) %></li>
        <li><%=link_to 'Logout', logout_path, method: 'delete'%></li>   
      <% else %> 
        <li class ="tab"><%= link_to 'Login', login_path %></li>
        <li class="tab"><%=link_to 'Signup', signup_path %></li> 
      <% end %>   
     </ul>
    </div>
  </nav>
</header>      
     
    
   

  